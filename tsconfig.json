{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsxImportSource": "@emotion/react",
    "moduleResolution": "node",
    "baseUrl": "."
    //    "paths": {
    //      "@/*": ["src/*"]
    //    }
  },
  "types": ["@emotion/react/types/css-prop"],
  "exclude": ["node_modules"],
  "include": ["src"]
}
